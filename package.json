{
  "name": "wayfinding-pathfinder",
  "version": "1.0.0",
  "description": "A modular and well-tested wayfinding pathfinder application for finding routes between fixtures through waypoints",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md}'",
    "validate": "npm run lint && npm run test"
  },
  "keywords": [
    "wayfinding",
    "pathfinding",
    "geojson",
    "mapping",
    "navigation"
  ],
  "author": "Wayfinding Team",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.0",
    "vite": "^5.0.4",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "deck.gl": "^9.0.0",
    "fuse.js": "^7.0.0",
    "maplibre-gl": "^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/wayfinding-pathfinder.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/wayfinding-pathfinder/issues"
  },
  "homepage": "https://github.com/your-org/wayfinding-pathfinder#readme"
}
